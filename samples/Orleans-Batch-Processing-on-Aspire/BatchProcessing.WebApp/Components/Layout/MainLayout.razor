@inherits LayoutComponentBase

<FluentLayout>
    <FluentHeader>
		<a href="/"><img style="height:2.5em" src='data:image/svg+xml;base64,' />
		</a>
    </FluentHeader>
    <FluentStack Class="main" Orientation="Orientation.Horizontal" Width="100%">
        <FluentBodyContent Class="body-content">
            <div class="content">
                @Body
            </div>
        </FluentBodyContent>
    </FluentStack>
    <FluentFooter>
        <FluentSpacer />
        <a href="https://learn.microsoft.com/en-us/dotnet/orleans/overview" target="_blank">About Orleans</a>
    </FluentFooter>
</FluentLayout>
